$TTL    604800
@       IN      SOA     services.mycluster.demo. admin.mycluster.demo. (
                  1     ; Serial
             604800     ; Refresh
              86400     ; Retry
            2419200     ; Expire
             604800     ; Negative Cache TTL
)

; name servers - NS records
    IN      NS      services

; name servers - A records
services.mycluster.demo.             IN      A      192.168.1.200

; OpenShift Container Platform Cluster - A records
bootstrap.lab.mycluster.demo.        IN      A      192.168.1.200
controlplane-1.lab.mycluster.demo.   IN      A      192.168.1.201
controlplane-2.lab.mycluster.demo.   IN      A      192.168.1.202
controlplane-3.lab.mycluster.demo.   IN      A      192.168.1.203
compute-1.lab.mycluster.demo.        IN      A      192.168.1.211
compute-2.lab.mycluster.demo.        IN      A      192.168.1.212
compute-3.lab.mycluster.demo.        IN      A      192.168.1.213

; OpenShift internal cluster IPs - A records
api.lab.mycluster.demo.              IN      A      192.168.1.200
api-int.lab.mycluster.demo.          IN      A      192.168.1.200
*.apps.lab.mycluster.demo.           IN      A      192.168.1.200
etcd-0.lab.mycluster.demo.           IN      A      192.168.1.201
etcd-1.lab.mycluster.demo.           IN      A      192.168.1.202
etcd-2.lab.mycluster.demo.           IN      A      192.168.1.203
console-openshift-console.apps.lab.mycluster.demo.     IN     A     192.168.1.200
oauth-openshift.apps.lab.mycluster.demo.               IN     A     192.168.1.200

; OpenShift internal cluster IPs - SRV records
_etcd-server-ssl._tcp.lab.mycluster.demo.    86400     IN    SRV     0    10    2380    etcd-0.lab
_etcd-server-ssl._tcp.lab.mycluster.demo.    86400     IN    SRV     0    10    2380    etcd-1.lab
_etcd-server-ssl._tcp.lab.mycluster.demo.    86400     IN    SRV     0    10    2380    etcd-2.lab

